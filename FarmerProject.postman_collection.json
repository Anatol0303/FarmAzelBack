							"    .tftable tr:hover {background-color:#e0ffff;}\r",
							"</style>\r",
							"\r",
							"<table class=\"tftable\" border=\"1\">\r",
							"    <tr>\r",
							"        <th>Quantity</th>\r",
							"        <th>Product</th>\r",
							"        <th>ID Type</th>\r",
							"        <th>ID Data</th>\r",
							"    </tr>\r",
							"    \r",
							"    {{#each response._surprizeBacks}}\r",
							"        <tr>\r",
							"            <td>{{quantity}}</td>\r",
							"            <td>{{product}}</td>\r",
							"            <td>{{_id.buffer.type}}</td>\r",
							"            <td>{{_id.buffer.data.[0]}}</td>\r",
							"        </tr>\r",
							"    {{/each}}\r",
							"</table>\r",
							"`;\r",
							"\r",
							"function constructVisualizerPayload() {\r",
							"    return {response: pm.response.json()}\r",
							"}\r",
							"\r",
							"pm.visualizer.set(template, constructVisualizerPayload());"
